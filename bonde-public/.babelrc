{
  "plugins": [
    [
      "react-intl",
      {
        "messagesDir": "./build/intl/messages"
      }
    ],
    [
      "inline-import",
      {
        "extensions": [
          ".css"
        ]
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ],
  "presets": [
    "next/babel"
  ]
}